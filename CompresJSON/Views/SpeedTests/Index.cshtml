                currentRequestID++;
                getManyItemsUnencrypted(take, currentRequestID, requests);
            }

            else {
                requestIDs = [];
                requestIsActive = false
                updateRequestingUI();
            }

            console.log(result);
            $('#unenc-' + take).children('pre').text(JSON.stringify(result, null, ' '))
